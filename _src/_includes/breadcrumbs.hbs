<div class="wrapper breadcrumb">
    <div class="container">
        <ul class="breadcrumb__items">
            {{#each items}}
            <li class="breadcrumb__item"><a class="breadcrumb__link" href="{{this.url}}">{{this.label}}</a></li>
            {{/each}}
            <li class="breadcrumb__item breadcrumb__item--current">{{currentPage}}</li>
        </ul>
    </div>
</div>
